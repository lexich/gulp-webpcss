{
  "name": "gulp-webpcss",
  "version": "1.1.0",
  "description": "A plugin for Gulp",
  "keywords": [
    "gulpplugin"
  ],
  "author": {
    "name": "Efremov Alex",
    "email": "lexich121@gmail.com",
    "url": "https://github.com/lexich"
  },
  "repository": "lexich/gulp-webpcss",
  "scripts": {
    "test": "npm run jshint && npm run jscs && istanbul test _mocha --report html -- test/*.js --reporter spec",
    "jshint": "./node_modules/.bin/jshint --reporter ./node_modules/jshint-stylish",
    "jscs": "node_modules/.bin/jscs .",
    "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "dependencies": {
    "through2": "^0.6.5",
    "webpcss": "^1.1.0"
  },
  "devDependencies": {
    "chai": "^3.2.0",
    "coveralls": "^2.11.4",
    "event-stream": "^3.3.1",
    "gulp-util": "^3.0.6",
    "istanbul": "^0.3.20",
    "jscs": "^2.1.1",
    "jshint": "^2.8.0",
    "jshint-stylish": "^2.0.1",
    "lodash": "^3.10.1",
    "mocha": "^2.3.2",
    "mocha-lcov-reporter": "0.0.2"
  },
  "engines": {
    "node": ">=0.10.0",
    "npm": ">=1.2.10"
  },
  "license": "MIT"
}
